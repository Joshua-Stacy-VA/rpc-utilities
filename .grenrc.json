{
    "dataSource": "milestones",
    "prefix": "",
    "groupBy": {
        "Enhancements": ["enhancement", "internal"],
        "Bug Fixes": ["bug"]
    },
    "milestoneMatch": "{{tag_name}}",
    "changelogFilename": "CHANGELOG.md",
    "template": {
        "issue": "- {{name}} [{{text}}]({{url}})"
    }
}